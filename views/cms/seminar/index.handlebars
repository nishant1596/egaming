<div class="container-fluid">
  <div class="row">
    <div class="col s12 l12 m12">
      <div class="cms-seminar-index-title">
        Seminars Lists
      </div><br>
      <div class="center">
        <a class="btn-floating  waves-effect waves-light red" href="seminar/add"><i class="material-icons">add</i></a>
      </div>
    </div>
  </div>
  <hr>
  <div class="container-fluid">
    {{#each seminar_list}}
    <div class="row">
      <div class="col s12">
        <div class="row" style="color:white">
          <div class="col s12 m2 l2 center">
            id: {{_id}}
          </div>
          <!-- <div class="col s12 m3 l3 center">
            published date:{{seminar_post_publish_date}}
          </div> -->
          <div class="col 12 m3 l3 center" style="font-size:20px">
            Title : {{seminar_title}}
          </div>
          <div class="col s12 m1 l1 center">
            <img src="/uploads/{{seminar_image}}" height="50px" width="50px">
          </div>
          <div class="col s6 m1 l1 center">
            <a href="seminar/edit/{{id}}" class="btn btn-sm blue">Edit</a>
          </div>
          <div class="col s6 m1 l1 center">
            <form class="" action="seminar/{{id}}?_method=DELETE" method="post">
              <input type="hidden" name="_method" value="DELETE">
              <input type="submit" class="btn btn-sm red" name="submit" value="DELETE">
            </form>
          </div>

        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
